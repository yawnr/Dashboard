<!DOCTYPE html>
<html>
<head>
  <title>Dashboard</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class="errors">
    <% if flash[:errors] %>
      <% flash[:errors].each do |error| %>
        <%= error %>
        <br/>
      <% end %>
    <% end %>
    </div>

    <%= yield %>

</body>
</html>
